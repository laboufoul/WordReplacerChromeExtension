{
    "name": "Text Replacement Extension",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Extension to exchange words on webpage",
    "action": {
        "default_icon": "icon.png"
    }, 
    "content-scripts":
    [
        {
            "matches": ["*://*/*"],
            "all-frames": true,
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}
